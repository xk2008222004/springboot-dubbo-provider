<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.dubbo.springbootdubbo.mapper.OrderMapper">

    <insert id="insertModel" parameterType="com.example.springbootdubbo.po.UserOrder" useGeneratedKeys="true" keyProperty="id">
        insert into user_order(order_no,create_time,user_name) values(#{orderNo},#{createTime},#{userName})
        <selectKey keyProperty="id" order="AFTER" resultType="java.lang.Integer">
            SELECT LAST_INSERT_ID()
        </selectKey>
    </insert>

    <delete id="deleteModel" parameterType="com.example.springbootdubbo.po.UserOrder">
        delete from user_order
        <where>
            <if test="id!=null">
                and id=#{id}
            </if>
            <if test="orderNo!=null and orderNo!=''">
                and order_no=#{orderNo}
            </if>
        </where>
    </delete>


</mapper>